{
  "name": "gseok-homepage-about-react",
  "description": "gseok homepage about area made by react",
  "version": "3.0.0",
  "author": "gyeongseok.seo <gseok.seo@gmail.com>",
  "license": "CC-BY-NC-4.0",
  "scripts": {
    "codelint": "eslint ./src --ext .js --ext .ts --ext .tsx -c .eslintrc.js",
    "clear": "rm -rf ./_site",
    "start": "yarn clear && webpack serve --env NODE_ENV=development --config webpack.config.js --progress --profile --color",
    "build:local": "yarn clear && webpack --env NODE_ENV=development --config webpack.config.js --progress --profile --color",
    "build:real": "yarn clear && webpack --env NODE_ENV=production --config webpack.config.js --progress --profile --color",
    "deployTo3.0": "node ./scripts/deploy3.0.js"
  },
  "dependencies": {
    "antd": "^2.9.1",
    "color": "^0.11.3",
    "core-js": "^3.14.0",
    "prop-types": "^15.5.6",
    "qrcode.react": "^0.6.1",
    "radium": "^0.18.0",
    "react": "^15.4.2",
    "react-dimensions": "2.0.0-alpha1",
    "react-dom": "^15.4.2",
    "react-graph-vis": "^0.1.1",
    "react-horizontal-timeline": "^1.1.1",
    "react-icons": "^2.0.1",
    "react-motion": "^0.4.2",
    "react-swipeable-views": "^0.12.0",
    "react-syntax-highlighter": "^5.3.0",
    "reflexbox": "^2.2.3"
  }
}
